{
    "name": "howyi/json-schema-mapper",
    "description": "JSON Schema(04, 06) map to PHP classes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name":  "Hayashi Takuya",
            "email": "qrbys@outlook.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php":                  ">=7.1",
        "symfony/console":      "~2.0 | ~3.0",
        "eloquent/enumeration": "^5.1",
        "twig/twig":            "^2.0",
        "howyi/evi":            "~1.0.0"
    },
    "autoload": {
        "psr-0": {
            "JsonSchemaMapper": "src/"
        }
    },
    "require-dev": {
        "symfony/console":           "~2.0 | ~3.0",
        "phpunit/phpunit":           "^6.2",
        "phpspec/prophecy":          "^1.7",
        "phpstan/phpstan":           "^0.8.0",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/var-dumper":        "^3.3",
        "satooshi/php-coveralls":    "^1.0"
    },
    "bin": ["bin/jsm"]
}
